  },
  "type": "module",
  "main": "dist/main.cjs",
  "module": "dist/module.mjs",
  "source": "src/index.js",
  "exports": {
    "import": "./dist/module.mjs",
    "require": "./dist/main.cjs"
  },
  "targets": {
    "main": {
      "engines": {
        "browsers": "Chrome 70"
      }
    }
  },
  "author": {
    "name": "Devon Govett",
    "email": "devongovett@gmail.com"
  },
